<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab5_javafx.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TextArea prefHeight="299.0" prefWidth="478.0" />
   <HBox prefHeight="25.0" prefWidth="478.0">
      <children>
          <Button onAction="#onHelloButtonClick" prefHeight="0.0" prefWidth="60.0" text="Import" textAlignment="CENTER">
            <HBox.margin>
               <Insets left="100.0" />
            </HBox.margin>
         </Button>
         <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" text="Clear" textAlignment="CENTER">
            <HBox.margin>
               <Insets left="150.0" />
            </HBox.margin>
         </Button>
      </children>
   </HBox>

          <Label fx:id="welcomeText" prefHeight="18.0" prefWidth="175.0" />
</VBox>
